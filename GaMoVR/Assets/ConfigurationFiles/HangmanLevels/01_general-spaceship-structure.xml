<Model>
    <Classes>
        <Class className="Spaceship" xPosition="2.5" yPosition="1" zPosition="0">
        </Class>
        <Class className="Hull" xPosition="5" yPosition="1" zPosition="0">
            <Property propertyName="length"></Property>
            <Property propertyName="width"></Property>
        </Class>
        <Class className="Engine" xPosition="5" yPosition="1" zPosition="5">
            <Property propertyName="powerOutput"></Property>
            <Property propertyName="fuelConsumption"></Property>
            <Method methodName="start()"></Method>
        </Class>
        <Class className="WeaponSystem" xPosition="5" yPosition="2" zPosition="2.5">
        </Class>
        <Class className="FuelTank" xPosition="0" yPosition="1" zPosition="0">
            <Property propertyName="capacity"></Property>
        </Class>
    </Classes>
    <Connectors>
        <DirectedAssociation origin="Spaceship" target="Hull" description="has"></DirectedAssociation>
        <DirectedAssociation origin="Spaceship" target="WeaponSystem" description="has"></DirectedAssociation>
        <DirectedAssociation origin="Spaceship" target="FuelTank" description="has"></DirectedAssociation>
        <DirectedAssociation origin="Spaceship" target="Engine" description="has"></DirectedAssociation>
    </Connectors>
</Model>